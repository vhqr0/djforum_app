{% extends "./base.djhtml" %}

{% block title %}
    DJForum User - {{ request.user }}
{% endblock %}

{% block body %}
    <form action="{% url 'djforum:userprofile-update' %}" method="POST">
        {% csrf_token %}
        <fieldset>
            {{ form.as_p }}
        </fieldset>
        <input class="btn btn-primary" type="submit" />
    </form>
{% endblock %}
